@model List<Doctor>

@foreach (var item in Model)
{
    <div class="col-md-4 col-sm-6 col-xs-12 doc">
        <div class="team-member">
            <img src="~/assets/images/doctor/@item.ImagePath" alt="doctor" class="img-responsive">
            <div class="contents text-center">
                <h4>@item.Name</h4>
                <p>@item.Department.Name</p>

                <div class="d-flex flex-column row-gap-1">
                    <a href="./doctor-detail.html" class="btn-main">read more</a>
                    <a href="./doctor-appointment.html" class="btn-main">appointment</a>
                </div>
            </div>
        </div>
    </div>
}